[user]
  name = Paul Haddad
  email = paulh16@gmail.com
[alias]
  tree = log --graph --oneline --decorate --all
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  ca   = commit --amend
[gitsh]
  noGreeting = true
  defaultCommand = status --short --branch && :echo
[gitsh "color"]
  default = cyan
  untracked = yellow
  modified = red
